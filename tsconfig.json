    "importHelpers": true,
    "esModuleInterop": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "target": "ES2022",
    "module": "es2020",
